{
  "layer": "top",
  "position": "top",
  "height": 0,
  "width": 1600,
  "spacing": 0,
  "margin-bottom": 0,
  "margin-top": 4,
  "margin-right": 500,
  "margin-left": 500,
  "modules-left":[
    "custom/applauncher",
    "tray",
    "custom/spotify",
    "cpu",
    "memory",
    "temperature"
  ],
  "modules-center":[
    "hyprland/workspaces"
  ],
  "modules-right":[
    "network",
    "bluetooth",
    "pulseaudio",
    "battery",
    "clock",
    "custom/powerbutton"
  ],

  "custom/applauncher":{
    "format": "Apps",
    "on-click": "pgrep wofi >/dev/null 2>&1 && killall wofi || wofi --show drun --location=top -y 285",
    "tooltip": false
},

"tray" : {
        "icon-size": 13,
        "spacing": 10,
        "cursor": true
    },

"custom/spotify":{
  "exec": "/usr/bin/python3 ~/.config/waybar/scripts/mediaplayer.py --player spotify",
  "format": "  {}",
  "return-type": "json",
  "on-click": "playerctl play-pause",
  "on-scroll-up": "playerctl next",
  "on-scroll-down": "playerctl previous"
},

"cpu":{
  "format":" {usage}%",
  "tooltip":false
},

"memory":{
  "format":" {}%",
  "tooltip":false
},
"temperature":{
  "format":" {}℃",
  "tooltip":false
},

"hyprland/workspaces":{
  "format":"{icon}",
  "all-outputs": false
  
},

"network":{
  "format-wifi":" {bandwidthDownBits}",
  "format-ethernet":" {bandwidthDownBits}",
  "format-disconnected":" No Network",
  "on-click": "nm-connection-editor",
  "interval": 5,
  "tooltop":false
},

"bluetooth":{
  "format-disabled":" Off",
  "format-disconnected":"No Signal",
  "on-click": "blueberry"
},

"pulseaudio": {
   
    "format": " {volume}%",
    "format-muted": " {volume}%",
    "on-click": "pavucontrol",
    "tooltip": false

},

"battery": {
    "tooltip":false,
    "states": {
      "warning": 30,
      "critical": 15
    },
    "format": "{icon} {capacity}%",
    "format-charging": "󰂄 {capacity}%",
    "format-plugged": "󰂄 {capacity}%",
    "format-alt": "{icon} {time}",
    "format-full": "󱈑 {capacity}%",
    "format-icons": [
      "󱊡",
      "󱊢",
      "󱊣"
    ]
    
  },

"clock":{
  "format": " {:%a %d/%m %H:%M}",
  "on-click": "gnome-calendar",
  "tooltip": false
  },

  "custom/powerbutton":{
  "format":"",
  "on-click": "swaync-client -t",
  "tooltip":false
},

"taskbar":{
  "format":"{icon}"
}

}